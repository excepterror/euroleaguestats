<Changelog>:
    name: 'changelog'
    Image:
        source: 'Images/About.jpg'
        keep_ratio: False
        allow_stretch: True
    RoundedRectLabelBtn:
        text: '[i]' + 'VIEW NOTES' + '[/i]'
        markup: True
        font_name: 'MyriadPro'
        font_size: sp(42)
        text_size: self.size
        halign: 'center'
        valign: 'center'
        size_hint: .7, .14
        color: 1, 1, 1, 1
        pos_hint: {'center_x': .5, 'center_y': .48}
        canvas.before:
            Color:
                rgba: 1, 1, 1, .2
            RoundedRectangle:
                segments: 70
                radius: dp(7), dp(7), dp(7), dp(7)
                pos: self.pos
                size: self.size
        on_press: root.view_notes()
    RoundedRectLabelBtn:
        text: '[i]' + 'PRIVACY POLICY' + '[/i]'
        markup: True
        font_name: 'MyriadPro'
        font_size: sp(42)
        text_size: self.size
        halign: 'center'
        valign: 'center'
        size_hint: .7, .14
        color: 1, 1, 1, 1
        pos_hint: {'center_x': .5, 'center_y': .3}
        canvas.before:
            Color:
                rgba: 1, 1, 1, .2
            RoundedRectangle:
                segments: 70
                radius: dp(7), dp(7), dp(7), dp(7)
                pos: self.pos
                size: self.size
        on_press: root.view_privacy_policy()
    Label:
        text: 'ELS v23.03.03'
        font_name: 'MyriadPro'
        font_size: sp(14)
        text_size: self.size
        halign: 'center'
        valign: 'middle'
        color: 1, 1, 1, 1
        pos_hint: {'center_x': .5, 'center_y': .02}